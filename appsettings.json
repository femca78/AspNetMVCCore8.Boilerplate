{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "SqlServerConnection": "Server=(localdb)\\mssqllocaldb;Database=MyDb;Trusted_Connection=True;",
    "PostgreSqlConnection": "Host=myserver;Database=mydb;Username=myuser;Password=mypassword",
    "MySqlConnection": "Server=myserver;Database=mydb;User=myuser;Password=mypassword;",
    "SqliteConnection": "Data Source=MyDb.db"
  },
  "DatabaseProvider": "SQLite" // Options: SqlServer, PostgreSQL, MySQL, SQLite, InMemory
}
